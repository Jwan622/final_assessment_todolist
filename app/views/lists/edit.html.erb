<div class="row">
  <%= form_for(@list, url: list_path, html: { class: "form-horizontal" }) do |f| %>
  <div class="control-group">
    <%= f.label :title, class: "control-label" %>
    <%= f.text_field :title, placeholder: @list.title %>
  </div>
  <div>
    <%= f.submit "Change Title" %>
  </div>
  <% end %>

</div>
