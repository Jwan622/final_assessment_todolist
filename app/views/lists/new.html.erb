<div class="row">
  <%= form_for(:list, url: lists_path, html: { class: "form-horizontal" }) do |f| %>
  <div class="control-group">
    <%= f.label :title, class: "control-label" %>
    <%= f.text_field :title, placeholder: "Title" %>
  </div>

  <div>
    <%= f.submit "Submit This Damn List" %>
  </div>
  <% end %>

</div>
